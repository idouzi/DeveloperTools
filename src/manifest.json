{
    "manifest_version": 2,

    "name": "开发者工具，Developer tools.",
    "description": "开发过程中高频使用的工具集合，High-frequency tools used in the development process.",
    "version": "1.0",

    "icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
    },
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", 

    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "开发者工具",
        "default_popup": "home.html"
    },

    "chrome_url_overrides": {
		"newtab": "tabs.html"
	},

    "permissions": [
        "activeTab",
        "storage"
    ]
}